/*comments*/
#comments p {
    font-size: .8em;
    color: gray;
    width: 70%; }

#comments p code {
    padding: 2em;
    color: gray; }

#comments {
    padding: 1.2em;
    width: 67%;
    float: right; }

.format-gallery .gallery dl.gallery-item img {
    max-height: 25em;
}

.page .article--content{
    max-width: 650px;
}
.page .article--title{
    padding-top: 0;
}
.page--title{
    padding-top: 0;
}

#events h2, #events h1 {
clear: both;
font-style: normal;
font-family: 'Oswald', sans-serif;
font-size: 1.4em;
}

p.events {
padding: .5em 0 1em;
font-style: italic;
font-size: .9em;
color: gray;
text-transform: lowercase;
display: inline-block;
}
#events p.description {
margin-bottom: 1em;
padding-bottom: 1em;
}

.events-page .page--title{
    margin-bottom: 20px;
}

/* =WordPress Core
-------------------------------------------------------------- */
.alignnone { margin: 5px 20px 20px 0; }

.aligncenter,
div.aligncenter {
    display: block;
    margin: 5px auto 5px auto; }

.alignright {
    float: right;
    margin: 5px 0 20px 20px; }

.alignleft {
    float: left;
    margin: 5px 20px 20px 0; }

.aligncenter {
    display: block;
    margin: 5px auto 5px auto; }

a img.alignright {
    float: right;
    margin: 5px 0 20px 20px; }

a img.alignnone { margin: 5px 20px 20px 0; }

a img.alignleft {
    float: left;
    margin: 5px 20px 20px 0; }

a img.aligncenter {
    display: block;
    margin-left: auto;
    margin-right: auto }

.wp-caption{
    background: #fff;
    border: 1px solid #f0f0f0;
    max-width: 96%; /* Image does not overflow the content area */
    padding: 5px 3px 10px;
    text-align: center; }

.wp-caption.alignnone { margin: 5px 20px 20px 0; }

.wp-caption.alignleft { margin: 5px 20px 20px 0; }

.wp-caption.alignright { margin: 5px 0 20px 20px; }

.wp-caption img, dl.gallery-item img {
    border: 0 none;
    height: auto;
    margin: 0;
    max-width: 98.5%;
    padding: 0;
    width: auto; }

.wp-caption p.wp-caption-text{
    font-size: 11px;
    line-height: 17px;
    margin: 0;
    padding: 0 4px 5px; }


/*
        RESPONSIVE LAYOUT
*/

@media screen and (max-width: 950px){
    .meta--image-logo{
        max-width: 68%;
    }
    .meta--menu{
        width: 100%;
    }
}

@media screen and 
(max-width: 769px) and
(min-width: 600px ){
    
    .main--sidebar .sidebar>li.widget{
        float:left;
        max-width: 350px;
        width: 48%;
        margin-right: 20px;
    }
}

@media screen and 
(max-width: 850px) and
(min-width: 769px ){
    .article--author-profile .avatar{
        display: none;
    }
    .authors--list, .authors--list-heading{
        display: none;
    }
    .article--single{
        margin-right: 0;
        padding: 0 15px;
        .article--information, .article--content-wrap{
            width: 100%;
            max-width: 450px;
            
            margin: 0 auto;
        }
        
    }
    
    .pull-sidenote-left {
        width:100%;
        padding: inherit;
        margin:0;
        text-align: left;
        font-size: .8em;
        position: static;
        font-style: italic;
        font-family: Georgia, serif;
        color:gray;
        line-height: inherit
    }

    .pull-sidenote-left:before {
        font-size: 1em;
        content: "-- ";
        display:inline;
        position: static;

    }

    .pull-sidenote-left:after {
        content: " ";
        display:inline;
        position:static;
    }
}

@media screen and 
(max-width: 860px) and
(min-width: 769px ){
    .main--sidebar{
        width: 40%;
    }
    .main--content{
        width: 60%;
    }
}

@media screen and (max-width: 769px){
    
    .meta--image-logo{
        display:none;
    }


    .meta--image-small{
        display:block;
        max-width: 100%;
    }
    
    .main--content{
        width: 100%;
    }
    .main--sidebar{
        width: 100%;
    }
    .sticky-nav{
        position: static;
    }
    #head-social{
        position: static;
        li{
            display: inline-block;
        }
        clear: both;
        text-align: center;
        margin: 0 auto;
    }
    .search-form{
        display: none;
    }
    .meta--menu{
        width: 100%;
    }
    .meta--image-logo{
        width: 100%;
        max-width: none;
    }
}

@media screen and (max-width: 600px){
    .article--author:before{
        content: "by ";
    }
    .article--single .article--meta{
        padding-bottom: 5px;
    }
    .article--author{
        text-align: left;
    }
    .article--author-profile .avatar{
        display: none;
    }
    .authors--list, .authors--list-heading{
        display: none;
    }
    .article--single{
        margin-right: 0;
        padding: 0 5px;
        .article--information, .article--content-wrap{
            width: 100%;
            max-width: 450px;
            float: right;
            margin: 0;
        }
        
    }
    
    .pull-sidenote-left {
        width:100%;
        padding: inherit;
        margin:0;
        text-align: left;
        font-size: .8em;
        position: static;
        font-style: italic;
        font-family: Georgia, serif;
        color:gray;
        line-height: inherit
    }

    .pull-sidenote-left:before {
        font-size: 1em;
        content: "-- ";
        display:inline;
        position: static;

    }

    .pull-sidenote-left:after {
        content: " ";
        display:inline;
        position:static;
    }
    

}

@media screen and (max-width: 500px) {
    
    .author--publications-wrap{
        display:none;
    }
    
    .author--info{
        width: 100%;
        text-align: center;
        float: none;
    }
    .author--summary{
        width: 100%;
        float: none;
    }
    
    .gallery #pager-nav a {
        padding: .125em 0.375em;
        font-size: 1em;
        margin: .125em;
    }

    .just-the-facts{
        width:100%;
    }
    .meta--mobile{
        display: inline;
    }
    
    #full-menu{
        display: none;
    }

    #header{
        margin-bottom: 0;
        padding-bottom: .25em;
    }

    .article--content {
        max-width:95%;
    }
    .search-form{
        display: none;
    }

    #comments{
        width:95%;
    }

    .footer .footer--attribution, .footer .footer--menu{
        text-align: center;
        display: block;
        float: none;
        img{
        margin: 0;
        
    }
    }
    .meta--menu{
        width: 100%;
    }
    #head-social{
        position: static;
        li{
        display: inline-block;
    }
    clear: both;
    text-align: center;
    margin: 0 auto;
    }

    nav.foot-links, #footer div.menu {
        max-width: 100%;
        font-size: .9em
    }

    #nextPrev{
        display:inherit;
        position: static;
        padding:.5em;
    }

    h1{
        font-size: 1.3em;
        padding:.5em 0;
    }

    #menu-mobile.mobile-only{
        text-align: center;
    }
    
    .article--list .article--no-image{
        border: none;
    }
    
    .article--list .article{
        border-bottom: 1px dashed $text-light-gray;
        margin: 0;
        padding: 10px 0;
    }
    .article--list .article .article--subtitle, .article--list .article .article--author{
        font-size: 14px;
    }
    .article--list .article-no-image .article--main-tag{
        padding: 0 0 0 5px;
    }
    .featured--primary .article {
        padding: 0;
        position: relative;
        margin-bottom: 20px;
        .article--meta{
            padding: 0;
        }
    }
    .featured--secondary .article {
        padding: 0;
        margin-right: 0;
        position: relative;
        margin-bottom: 20px;
        .article--content{
            display: none;
        }
    }
    .article--list .article--with-image{
        .article--image-wrap{
            max-width: 30%;
            float: left;
            padding-right: 10px;
        }
        .article--meta-container{
            position: relative;
            overflow: auto;
            max-height: none;
            img{
                width: auto;
                min-width: 0;
                height: auto;
                min-height: 0;
                max-width: 100%;
            
            }
        }
        .article--main-tag{
            position: static;
            padding: 0;
            background: none;
            display: block;
            width: auto;
            max-width: none;
            min-width: 0;
        }
        .article--image-wrap{
            position: relative;
            border: none;
        }
        .article--meta{
            float: left;
            max-width: 65%;
            background: none;
            position: static;
            border: none;
            a{
                color: $text-gray;
            }
        }
        .article--heading{
            margin-top: .25em;
        }
        .article--content{
            display: none;
        }
        .article--author{
            display: none;
        }
    }
    .article--list .article .article--title, .article--list .article .article--subtitle, .article--list .article .article--author{
        max-width: 100%;
        color: $text-gray;
        text-align: left;
        padding-left: 0;
    }


    h4.mobile-only, p.mobile-only{
        font-size: .875em;
    }

    .article--list .article{
        width:100%;
        max-width:100%;
        border-radius: 0;
        box-shadow: none;
        float: none;
    }
    .main--content, .main--sidebar{
        width: 100%;
    }

    .sticky-nav {
        position: static;
    }

    article.widget{
        overflow: hidden;
    }
    .featured--sidebar .article--meta-container{
        width: 40%;
    }
    .featured--secondary-wrap{
        width: 100%;
        float: none;
    }

}

@media screen and (min-width: 1100px) {
    .constrain{
        max-width: 1080px;
    }
    .meta{
        margin-top: 2em;
    }

}

@media screen and (min-width: 1400px) {
    .constrain{
        max-width: 1280px;
    }
}

