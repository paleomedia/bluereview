/*
    Reset borrowed from -
    http://meyerweb.com/eric/tools/css/reset/
   v2.0 | 20110126
   License: none (public domain)
*/
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed,
figure, figcaption, footer, header, hgroup,
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline; }

/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section { display: block; }

body { line-height: 1; }

ol, ul { list-style: none; }

blockquote, q { quotes: none; }

blockquote:before, blockquote:after,
q:before, q:after {
    content: '';
    content: none; 
}

table {
    border-collapse: collapse;
    border-spacing: 0; 
}

/* Page Layout and colors */

$white: #fdfdfd;
$background-gray: #e6e7e9;
$text-gray: #414042;
$text-light-gray: #808080;
$text-lightest-gray: #C0C0C0;
$tag-pink: #c40532;

$tag-font: 'Carrois Gothic SC', sans-serif;
$sans-serif: 'Oswald', sans-serif;


$border-pixels: 0px;

/* Reusable Gradient */
%gradient {
    background: black;
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  rgba(0,0,0,0) 0%, rgba(0,0,0,0.85) 43%, rgba(0,0,0,0.6) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,0)), color-stop(43%,rgba(0,0,0,0.85)), color-stop(100%,rgba(0,0,0,0.6))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.85) 43%,rgba(0,0,0,0.6) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.85) 43%,rgba(0,0,0,0.6) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.85) 43%,rgba(0,0,0,0.6) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.85) 43%,rgba(0,0,0,0.6) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#99000000',GradientType=0 ); /* IE6-8 */
}

%clearfix:after, .widget:after{
content: ".";
display: block;
clear: both;
visibility: hidden;
line-height: 0;
height: 0;
}

.relative{
    position: relative;
}

/* Header CSS */
@import "header";
/* Article styles */
@import "article";
/* Features for home page */
@import "featured";
/* Author Styles */
@import "author";
/* Gallery Styles   */
@import "gallery";

body{
    background: $white;
    color: $text-gray;
}
h1, h2, h3, h4{

    -webkit-hyphens: auto;
    -moz-hyphens: auto;
    hyphens: auto;
}

.main--content{
    width: 65%;
    float: left;
    position: relative;
}

.main--sidebar{
    width: 35%;
    float: left;
}
.main--content-wrap{
    @extend %clearfix;
}

.footer{
    min-height: 2em;
    padding-top: 1.625em;
    padding-left: .5em;
    
    .footer--attribution{
        float:right;
        text-align:right;
        max-height: 100%;
        display:inline;
        vertical-align: top;
        img{
            max-height: 30px;
            padding: 0 15px;
            margin-top: -10px;
        }
        a{
            vertical-align: top;
        }
    }
    .footer--menu ul{
        /*float: left;*/
    }
    .footer--menu li{
        display: inline;
        display: inline-block;
    }
    .footer--menu a{
        text-decoration: none;
        font-size: 1em;
        text-transform: uppercase;
        color:black;
    }

    .footer--menu li:after{
        content: " | ";
        padding:.5em;
    }

    .footer--menu li:last-child:after{
        content: "";
    }

    footer--menu{
        max-width: 65%
    }
}

.constrain {
    width: 100%;
    max-width: 960px;
    margin: 0 auto; 
    clear:both;
}

#infscr-loading {
    position: absolute;
    bottom: 5em;
    text-align: center;
    border-radius: 7px;
    background: #000000;
    width: 30%;
    margin: auto;
    left: 30%;
    color: white;
    padding: 2em;
    z-index: 999;
}

.showMe{
    z-index:999;
}

em em {
    font-style: normal;
}

.pull-sidenote-left {
    margin: 5px;
    text-align: left;
    font-size: .8em;
    color: black;
    position: absolute;
    left: 0;
    width: 16%; 
    margin-top: -1em;
    display: inline;
    font-family: 'Oswald', sans-serif;

}

.pull-sidenote-left:before{
    font-family: georgia, serif;
    content:"}";
    font-size: 8em;
    width:0;
    position:absolute;
    top:20%;
    right: 0;
    margin-top: -.4em;
}

/* search form */

li.search-form{
    max-width:1.6em;
    position:relative;
}

input.search-but {
    border: none;
    background: url('images/search.png') #7f7f7f center no-repeat;
    font-size: 1em;
    text-indent: -9999px;
    width: 1.25em;
    height: 1.375em;
    cursor: pointer;
    color: transparent;
    text-transform: capitalize; 
}

input.search-sub {
    color:black;
    border: none;
    background: #ffffff center no-repeat;
    font-size: 1em;
    width: 17%;
    height: 1.6em;
    cursor: pointer;
    text-transform: capitalize; 
}

ul.article>li{
    padding-bottom:.625em;
    margin-bottom:.625em;
    background: url('images/black-dots.png') repeat-x bottom;
}

ul.article>li:last-of-type {
    padding-bottom:1em;
    margin-bottom:.625em;
    border-bottom: none;
    background: none;
}

ul.sidebar>li{
    margin-bottom: 20px;
}

.widgettitle{
    width: 100%;
    background-color: #7f7f7f;
    text-align: center;
    color: $white;
    padding: 10px 0;
    margin-bottom: 10px;
}

ul.sidebar li ul{
    margin-left: .5em;
}

li.tweet{
    padding:.625em 0;
    background: url('images/black-dots.png') repeat-x bottom;
}


sidebar li.tweet{
    padding:.625em 0;
    background: url('images/black-dots.png') repeat-x bottom;
}

#done.reallyDone { background: none; }

#footer.reallyDone {
    box-shadow: none;
    -webkit-box-shadow: none;
    -moz-box-shadow: none; }

.license{
    position:absolute;
    right:.5em;
}

.rel{
    position:relative;
}

/* Text Styles */

h1{
    font-family: 'Oswald', sans-serif;
    color: black;
    font-size: 1.5em;
    padding:1em 0 .25em;
    line-height: 1.25em;
}

time{
    font-style: italic;
    font-size: .9em;
    font-family: 'Georgia', serif;
    padding-top:.25em;
}

h2{
    font-size: 1.25em;
    font-family: 'Georgia', serif;
    padding-top:.25em;
}

.article .article--content ul li {
    list-style-type:disc;
    padding: 0 0 .5em .5em;
}

.article .article--content ul {
    padding: 0 1em .5em 1em;
    margin: 0 0 0 1em;
}

.article .article--content ul li li {
    list-style-type:circle;
}

.article .article--content ul li li li {
    list-style-type:square;
}

.article .article--content ol li{
    list-style-type:decimal;

}

.hide-text{
    display:none;
}

.hidden{
    display:none;
}

a{
    color: $text-gray;
    text-decoration: none;
}

 .article--content a{
    color: #12c;
}

a:hover{
    text-decoration: underline;
}

.clear{
    clear:both;
}

.right{
    float:right;
}

.mobile-only{
    display:none;
}
